// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users{
  id         Int           @id @default(autoincrement())
  name       String
  email      String        @unique
  password   String
  @@map("users")
}
model Pets{
  id         Int            @id @default(autoincrement())
  name       String
  birthdate  String
  genre      String
  breed      String
  @@map("pets")
}

enum GenreType{
    macho
    femea
}