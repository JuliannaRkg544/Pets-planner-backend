    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "joi": "^17.6.2",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.5-lts.1",
    
    "nodemon": "^2.0.20",
    "prisma": "^4.4.0",
    "supertest": "^6.3.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
